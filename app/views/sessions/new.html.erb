<h1>Log in Here</h1>
<%=flash[:message] if flash[:message]  %>


<%=form_for :student  do |f|%>
    <%=f.label :username%>
    <%=f.text_field :username%>
    
    <%=f.label :password%>
    <%=f.password_field :password%>
    <%=f.submit "Log In"%>

        <%end%>
        <br><br>
<%= link_to 'Sign Up', signup_path%>
<%=link_to 'Log in', login_path %>
<%= button_to 'Log in with Google', '/auth/google_oauth2'%>
